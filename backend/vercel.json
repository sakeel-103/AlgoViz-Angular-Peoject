{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "headers": {
        "Content-Type": "application/json"
      },
      "dest": "/api/404"
    }
  ]
}